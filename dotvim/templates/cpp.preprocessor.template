$-------------------------------------------------------------------------
== preprocessor.include-global ==
#include <<CURSOR>>
$-------------------------------------------------------------------------
== preprocessor.include-local ==
#include "<CURSOR>"
$-------------------------------------------------------------------------
== preprocessor.define ==
#define <CURSOR>
$-------------------------------------------------------------------------
== preprocessor.undefine ==
#undef <CURSOR>
$-------------------------------------------------------------------------
== preprocessor.if-endif ==
#if |?CONDITION:u|
<CURSOR><SPLIT>
#endif // #if |CONDITION|
$-------------------------------------------------------------------------
== preprocessor.if-else-endif ==
#if |?CONDITION:u|
<CURSOR><SPLIT>
#else      // not |CONDITION|
<+ELSE PART+>

#endif     // #if |CONDITION|
$-------------------------------------------------------------------------
== preprocessor.ifdef-else-endif ==
#ifdef |?CONDITION:u|
<CURSOR><SPLIT>
#else      // not defined |CONDITION|
<+ELSE PART+>
#endif     // #ifdef |CONDITION|
$-------------------------------------------------------------------------
== preprocessor.ifndef-else-endif ==
#ifndef |?CONDITION:u|
<CURSOR><SPLIT>
#else      // defined |CONDITION|
<+ELSE PART+>
#endif     // #ifndef |CONDITION|
$-------------------------------------------------------------------------
== preprocessor.ifndef-def-endif ==
#ifndef |?BASENAME:L|_INCLUDED_
#define |BASENAME|_INCLUDED_
<CURSOR><SPLIT>
#endif // #ifndef |BASENAME|_INCLUDED_
$-------------------------------------------------------------------------
== preprocessor.error ==
#error "<CURSOR>"
$-------------------------------------------------------------------------
== preprocessor.line ==
#line   <CURSOR>            //
$-------------------------------------------------------------------------
== preprocessor.pragma ==
#pragma <CURSOR>            //
$-------------------------------------------------------------------------
